<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>Endpoints  configuration</comment>
     <entry key="DatabaseConnectionString">sqlserver://localhost;databaseName=gerrithub-gerrit-dataset;integratedSecurity=true;</entry>
    <entry key="GerritEndpoint">https://review.gerrithub.io/</entry>
    <entry key="ChangesQuery">status:open</entry>
    <entry key="DownloadPause">5000</entry>
    <entry key="Start">847</entry>
    <entry key="Stop">300000</entry>
    <entry key="Limit">400</entry>
    <entry key="MiningMode">simple</entry>
    <entry key="DownloadAttemptsLimit">3</entry>
</properties>
